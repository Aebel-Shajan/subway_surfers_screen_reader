{
	"name": "Subway Surfers Screen Reader",
  "version": "0.0.2",
  "manifest_version": 3,
	"description": "Screen reader with integrated subway surfers gameplay in the background.",
	"icons": {
		"128": "images/genz-screenreader-logo.png"
	},
	"permissions": ["sidePanel", "tts", "scripting", "contextMenus", "activeTab"],
	"action": {
		"default_title": "Click to open panel"
	},
	"side_panel": {
		"default_path": "sidePanel.html"
	},
	"background": {
		"service_worker": "service-worker.js",
		"type": "module"
	}
}